{
    "rewrites": [
        {
            "source": "/ffmpeg-core.js",
            "destination": "/ffmpeg-core.js"
        },
        {
            "source": "/ffmpeg-core.wasm",
            "destination": "/ffmpeg-core.wasm"
        },
        {
            "source": "/ffmpeg-core.worker.js",
            "destination": "/ffmpeg-core.worker.js"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                }
            ]
        }
    ]
}